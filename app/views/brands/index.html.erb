<h1 class="text-center">All Brands</h1>
<p class="text-center"> <%= link_to "Add Brand", new_brand_path %> </p>
<p class="text-center"> <%= link_to "Add Model", new_model_path %> </p>
<div class="grid grid-cols-5 mx-20 gap-20">
<% @brands.each do |brand| %>
  <div class="text-center"> <%= link_to brand.brand, "/brands/#{brand.id}" %> 
    <%= link_to "Edit", edit_brand_path(brand) %>
  </div>
<% end %>
</div>